{
  "name": "back-end",
  "version": "1.0.0",
  "description": "https://docs.docker.com/compose/install/",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "ws": "node server-ws.js",
    "lint": "npx eslint .",
    "format": "npx eslint . --fix",
    "prettier": "npx prettier -c .",
    "prettier:format": "npx prettier -w .",
    "test": "jest --reporter=list --watch",
    "test:watch": "jest --reporter=list --coverage",
    "start-dev": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test:int": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-websocket-mock": "^2.2.0",
    "mock-socket": "^9.0.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.8.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "tap": "^15.0.9"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-signature": "^1.1.0",
    "docker": "^1.0.0",
    "docker-compose": "^0.23.8",
    "dotenv": "^8.6.0",
    "fastify": "^3.15.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^5.2.0",
    "fastify-plugin": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "mongoose": "^5.12.8",
    "pg": "^8.6.0",
    "ws": "^7.4.5",
    "yarn": "^1.22.10"
  },
  "directories": {
    "doc": "doc"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": []
}
